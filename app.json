{
  "expo": {
    "name": "Asaph",
    "slug": "Asaph",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/Revived.png",
    "scheme": "asaph",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "",
        "backgroundColor": ""
      },
      "edgeToEdgeEnabled": true,
      "package": "com.jamison_lii.Asaph"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/Revived.png",
          "imageWidth": 400,
          "imageHeight": 400,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
